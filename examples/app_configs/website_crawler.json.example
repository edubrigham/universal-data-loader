{
  "sources": [
    {
      "type": "url",
      "path": "https://your-website.com/docs",
      "output_prefix": "main_docs"
    },
    {
      "type": "url",
      "path": "https://your-website.com/blog",
      "output_prefix": "blog_posts"
    },
    {
      "type": "url",
      "path": "https://your-website.com/faq",
      "output_prefix": "faq"
    },
    {
      "type": "url",
      "path": "https://your-website.com/support",
      "output_prefix": "support_docs"
    }
  ],
  "loader_config": {
    "output_format": "documents",
    "chunking_strategy": null,
    "include_metadata": true,
    "min_text_length": 100,
    "remove_headers_footers": true
  },
  "output": {
    "base_path": "website_content",
    "separate_by_source": true,
    "merge_all": true,
    "filename_template": "{source_name}_content"
  },
  "max_workers": 4,
  "continue_on_error": true,
  "verbose": true
}